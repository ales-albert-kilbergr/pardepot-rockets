{
  "name": "rockets",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn intl:compile && nx serve",
    "build": "yarn intl:compile && nx build",
    "test": "yarn intl:compile && nx test",
    "intl:extract": "formatjs extract 'apps/rockets-browser-app/src/**/*.ts*' --ignore='**/*.d.ts' --out-file apps/rockets-browser-app/src/intl/en-us.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile:en": "formatjs compile apps/rockets-browser-app/src/intl/en-us.json --ast --out-file apps/rockets-browser-app/src/assets/compiled-intl/en-us.json",
    "intl:compile:de": "formatjs compile apps/rockets-browser-app/src/intl/de-de.json --ast --out-file apps/rockets-browser-app/src/assets/compiled-intl/de-de.json",
    "intl:compile:fr": "formatjs compile apps/rockets-browser-app/src/intl/fr-fr.json --ast --out-file apps/rockets-browser-app/src/assets/compiled-intl/fr-fr.json",
    "intl:compile": "yarn intl:compile:fr & yarn intl:compile:de & yarn intl:compile:en"
  },
  "private": true,
  "dependencies": {
    "core-js": "^3.6.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-intl": "^6.1.1",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.0",
    "@nrwl/cli": "13.10.6",
    "@nrwl/cypress": "13.10.6",
    "@nrwl/eslint-plugin-nx": "13.10.6",
    "@nrwl/jest": "13.10.6",
    "@nrwl/linter": "13.10.6",
    "@nrwl/react": "13.10.6",
    "@nrwl/web": "13.10.6",
    "@nrwl/workspace": "13.10.6",
    "@testing-library/react": "13.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "babel-jest": "27.2.3",
    "babel-plugin-formatjs": "^10.3.28",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "jest": "27.2.3",
    "nx": "13.10.6",
    "prettier": "^2.5.1",
    "react-test-renderer": "18.0.0",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2"
  }
}
